                                <li><a href="#">German</a></li>
                                <li><a href="#">Spanish</a></li>
                            </ul>
                        </li>*@
                    <li class="menu_item has-children">
                        <a href="#">
                            My Account
                            <i class="fa fa-angle-down"></i>
                        </a>
                        <ul class="menu_selection">
                            <li><a href="/account/login"><i class="fa fa-sign-in" aria-hidden="true"></i>Sign In</a></li>
                            <li><a href="/account/register"><i class="fa fa-user-plus" aria-hidden="true"></i>Register</a></li>
                        </ul>
                    </li>
                    <li class="menu_item"><a href="/trang-chu">Trang chủ</a></li>
                    <li class="menu_item"><a href="/post/gioi-thieu">Giới thiệu</a></li>
                    <li class="menu_item"><a href="/san-pham">Sản phẩm</a></li>
                    <li class="menu_item"><a href="/lien-he">Liên hệ</a></li>
                    <li class="menu_item"><a href="/tin-tuc">Tin tức</a></li>
                    <li class="menu_item"><a href="/post/khuyen-mai">Khuyến mãi</a></li>
                </ul>
            </div>
        </div>

        @RenderBody();

        <!-- Footer -->

        <footer class="footer-container">
            <div class="container">
                <div class="row">
                    <div class="col-3">
                        <h5 class="footer_title">Về Coffour</h5>
                        <ul class="footer_list">
                            <li class="footer-item"><a class="footer-link" href="/post/gioi-thieu">Giới thiệu</a></li>
                            <li class="footer-item"><a class="footer-link" href="/post/huong-dan-mua-online">Hướng dẫn mua online</a></li>
