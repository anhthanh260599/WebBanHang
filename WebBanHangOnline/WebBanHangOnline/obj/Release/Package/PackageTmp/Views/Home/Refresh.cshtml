@model WebBanHangOnline.Models.ThongKeViewModelString
@if (Model != null)
{
    <div class="thong-ke">
        <table class="table">
            <tr>
                <td>Hôm nay</td>
                <td>@Model.HomNay</td>
            </tr>

            <tr>
                <td>Hôm qua</td>
                <td>@Model.HomQua</td>
            </tr>

            <tr>
                <td>Tuần này</td>
                <td>@Model.TuanNay</td>
            </tr>

            <tr>
                <td>Tuần trước</td>
                <td>@Model.TuanTruoc</td>
            </tr>

            <tr>
                <td>Số người online</td>
                <td>@ViewBag.Visitors_online</td>
            </tr>

         

        </table>
    </div>
}    
